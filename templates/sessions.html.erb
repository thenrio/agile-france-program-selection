<html>
<head>
  <link rel='stylesheet' media='all' href='<%= "#{File.dirname(File.expand_path __FILE__)}/templates/css/program.css" %>'>
</head>

<body>
<div id='session'>
  <% sessions.each_with_index do |session, index| %>
    <div id='<%= session.category %>'>
      <h3><%= session.title %></h3>

      <h4><%= session.speaker.full_name %></h4>

      <h4>level : <%= session.level %></h4>
    </div>
  <% end %>
</div>
</body>
</html>
